class Program
{
  int a[5];
  int b[5];
  int c[5];
  int n;

  void vecadd()
  {
    int i;
    for i=0,n
    {
      c[i] = a[i] + b[i];
      i = i + 1;
    }
  }

  void main()
  {
    int i;
    n = 5;
    for i=0,n
    {
      a[i] = n-i;
      b[i] = i;
      i =  i + 1;
    }
    vecadd();
    for i=0,n
    {
      callout("printf", "%d + %d = %d\n", a[i], b[i], c[i]);
      i = i + 1;
    }
    callout("printf", "\n");
  }
}
